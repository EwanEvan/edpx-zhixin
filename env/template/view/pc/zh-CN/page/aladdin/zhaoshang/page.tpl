{%extends 'base.tpl'%} {%block name='content'%}{%$FE_GBVAR.wrapper_prefix%}<style>.op-zhaoshang .op-zhaoshang-rows{padding-top:5px;font-size:13px;color:#000}.op-zhaoshang .op-zhaoshang-rows .row{padding-top:5px}.op-zhaoshang .op-zhaoshang-info{padding-top:5px}.op-zhaoshang .op-zhaoshang-rows .row a{font-family:"宋体","simsun";font-size:13px;color:#00C;margin-right:12px}.op-zhaoshang .op-zhaoshang-more{padding-top:5px;font-size:13px;font-weight:bold;color:#000}.op-zhaoshang .op-zhaoshang-more a{font-weight:normal;padding-left:20px;background-position:0 center;background-r{%*i*%}epeat:no-repeat}.op-zhaoshang .op-zhaoshang-more span.sep{padding:0 10px;color:#d8d8d8;font-size:13px}</style>{%fe_fn_title_prefix%}{%fe_fn_title title="{%$tplData.title%}" url="{%$tplData.url%}"%}{%fe_fn_title_suffix title="{%$tplData.title%}" url="{%$tplData.url%}"%}<div class="op-zhaoshang"> <div class="op-zhaoshang-rows"></div> <div class="op-zhaoshang-info">{%fe_fn_showurl showurl="{%$tplData.showurl%}" date="{%$tplData.date%}" showlamp="{%$tplData.showlamp%}"%}</div> <div class="op-zhaoshang-more"></div></div><script>{%$rows=$tplData.rows.row%}{%$row_count=count($rows)%}{%$more=$tplData.more.morelink%}{%$more_count=count($more)%}A.setup({"ecom_zs_2013_data_rows": [{%for $loop_i=0 to $row_count-1%}{%$link_count=count($rows[$loop_i]['link'])%}{title: "{%$rows[$loop_i]['title']%}",links: [{%for $loop_j=0 to $link_count-1%}{title: "{%$rows[$loop_i]['link'][$loop_j]['linkcontent']%}",url: "{%$rows[$loop_i]['link'][$loop_j]['linkurl']%}"}{%if $loop_j!=$link_count-1%},{%/if%}{%/for%}]}{%if $loop_i!=$row_count-1%},{%/if%}{%/for%}],"ecom_zs_2013_data_more":[{%for $loop_i=0 to $more_count-1%}{title: "{%$more[$loop_i]['text']%}",url: "{%$more[$loop_i]['url']%}",icon: "{%$more[$loop_i]['iconsrc']%}"}{%if $loop_i!=$more_count-1%},{%/if%}{%/for%}]});</script><script>A.init(function(){var _this = this;var ECOM_ZS_2013_DATA={};ECOM_ZS_2013_DATA["rows"]=this.data["ecom_zs_2013_data_rows"];ECOM_ZS_2013_DATA["more"]=this.data["ecom_zs_2013_data_more"];var Ecom_zs_2013 = {init:function(){this.displayRows();if(ECOM_ZS_2013_DATA["more"].length > 0){this.displayMore();}},displayRows:function(){_this.find('.op-zhaoshang-rows').eq(0).html(this._createRows()) ;},displayMore:function(){_this.find('.op-zhaoshang-more').eq(0).html(this._createMore());{%*i*%}},_createMore:function(){var a,tx,ic,ur,html = '更多招商加盟选择：';for (var i = 0; i < ECOM_ZS_2013_DATA['more'].length; i++) {ur = ECOM_ZS_2013_DATA['more'][i]['url'];ic = ECOM_ZS_2013_DATA['more'][i]['icon'];tx = ECOM_ZS_2013_DATA['more'][i]['title'];a = '<a target="_blank" href="'+ ur +'" style="background-image:url('+ ic +');">'+ tx +'</a>';if(i !== ECOM_ZS_2013_DATA['more'].length-1){a += '<span class="sep">|</span>';}html = html + a;};return html;},_createRows:function(){var rowData,html = '';for {%*i*%}(var i = 0; i < ECOM_ZS_2013_DATA['rows'].length; i++) {rowData = ECOM_ZS_2013_DATA['rows'][i];html = html + this._createRow(rowData);};return html;},_createRow:function(rowData){var tx,ur;var html = '<div class="row"><label>'+ rowData['title'] +'</label>';for (var i = 0; i < rowData['links'].length; i++) {tx = rowData['links'][i]['title'];ur = rowData['links'][i]['url'];html = html + '<a target="_blank" href="'+ ur +'">'+ tx +'</a>';};html = html + "</div>";return html;}};Ecom_zs_2013.init();}){%*i*%};</script>{%$FE_GBVAR.wrapper_suffix%}{%/block%}
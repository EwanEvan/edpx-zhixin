{%extends 'c_base.tpl'%} {%block name='title'%}{%/block%}{%block name='data_modifier'%}{%$extData.feData.hasBorder=true%}{%/block%}{%block name='content'%}<div class="c-row"> <div class="c-span3"><i class="c-tool c-tool-zip"></i></div> <div class="c-span21 c-span-last"> {%if $tplData.linktitle%} <a href="%$tplData.url%}" target="_blank" >{%$tplData.title|escape:'html'%}</a> {%else%} <span class="op_zipcode_title">{%$tplData.title|escape:'html'%}</span> {%/if%} <form target="_blank" action="{%$tplData.input[0]['action']|escape:'html'%}" onsubmit="return false;" class="op_zipcode_form c-gap-top" accept-charset="gb2312"> <input type="text" name="{%$tplData.input[0]['name']|escape:'html'%}" value="{%$tplData.input[0]['tip']|escape:'html'%}" class="op_zipcode_txt op_zipcode_tip c-input c-input-large c-gap-right" /><a href="javascript:void(0)" class="op_zipcode_btn c-btn c-btn-primary">查询</a>{%$loop_maxcount=count($tplData.param)%}{%for $op_loop_count=0 to 5%}{%if $op_loop_count<$loop_maxcount%}<input type="hidden" name="{%$tplData.param[$op_loop_count]['name']|escape:'html'%}" value="{%$tplData.param[$op_loop_count]['value']|escape:'html'%}" />{%else%}{%break%}{%/if%}{%/for%}<div style="clear:both;"></div> </form> <p class="op_zipcode_result"> </p> </div></div><div class="c-gap-top-small">{%if $tplData.link[0]['label']%}<span class="op_zipcode_linklabel">{%$tplData.link[0]['label']|escape:'html'%}</span>{%/if%}{%if $tplData.link[0]['content']%}<a href="{%$tplData.link[0]['url']|escape:'html'%}" target="_blank">{%$tplData.link[0]['content']|escape:'html'%}</a>{%/if%}</div>{%/block%}